<div class="box box-primary">
    <div class="box-header">
        <h3 class="box-title">Data Personal</h3>
        <div class="pull-right box-tools">
            <button class="btn btn-primary btn-sm" data-widget="collapse"><i class="fa fa-minus"></i></button>
            <button class="btn btn-primary btn-sm" data-widget="remove"><i class="fa fa-times"></i></button>
        </div><!-- /. tools -->
    </div>
    <!-- /.box-header -->
    <!-- form start -->
    <form method="get">
        <div class="col-lg-6 box-body">
            <div class="form-group">
                <label for="nama_lengkap">Nama Lengkap</label>
                <input type="text" class="form-control" name="nama_lengkap" id="nama_lengkap" placeholder="Nama Lengkap">
            </div>
            <div class="form-group">
                <label for="tempat_lahir">Tempat/Tgl Lahir</label>
                <input type="text" class="form-control" name="tempat_lahir" id="tempat_lahir" placeholder="Tempat Lahir">
            </div>
            <div class="form-group">
                <label for="sifat_dominan">Sifat Dominan</label>
                <input type="text" class="form-control" name="sifat_dominan" id="sifat_dominan" placeholder="Sifat Dominan">
            </div>
            <div class="form-group">
                <label for="alamat_rumah">Alamat Rumah</label>
                <input type="text" class="form-control" name="alamat_rumah" id="alamat_rumah" placeholder="Alamat Rumah">
            </div>
            <div class="form-group">
                <label for="kuliah">Kuliah di/Jurusan/Semester</label>
                <input type="text" class="form-control" name="kuliah" id="kuliah" placeholder="Kuliah di/Jurusan/Semester">
            </div>
            <div class="form-group">
                <label for="pekerjaan">Pekerjaan Sambilan</label>
                <input type="text" class="form-control" name="pekerjaan" id="pekerjaan" placeholder="Pekerjaan Sambilan">
            </div>
            <div class="form-group">
                <label for="target">Target 1 Tahun Kedepan</label>
                <input type="text" class="form-control" name="target" id="target" placeholder="Target 1 Tahun Kedepan">
            </div>
        </div>


        <div class="col-lg-6 box-body">
            <div class="form-group">
                <label for="nama_panggilan">Nama Panggilan</label>
                <input type="text" class="form-control" id="nama_panggilan" name="nama_panggilan" placeholder="Nama Panggilan">
            </div>
            <div class="form-group">
                <label for="jenis_kelamin">Jenis Kelamin</label>
                <input type="text" class="form-control" name="jenis_kelamin" id="jenis_kelamin" placeholder="Jenis Kelamin">
            </div>
            <div class="form-group">
                <label for="suku">Suku</label>
                <input type="text" class="form-control" name="suku" id="suku" placeholder="Suku">
            </div>
            <div class="row form-group">
                <div class="col-xs-6">
                    <label>Email</label>
                    <input type="text" class="form-control" name="email" id="contact" placeholder="Email">
                </div>
                <div class="col-xs-6">
                    <label>Nr. Tlpn</label>
                    <input type="text" class="form-control" name="nr_tlpn" id="contact" placeholder="Nr. Tlpn">
                </div>
            </div>
            <div class="form-group">
                <label for="hobi">Hobi</label>
                <input type="text" class="form-control" name="hobi" id="tokenfield" value="" placeholder="Tulis Lalu Tab"/>
            </div>
            <div class="form-group">
                <label for="penghasilan">Penghasilan per-bulan</label>
                <input type="text" class="form-control" name="penghasilan" id="penghasilan" placeholder="Penghasilan per-bulan">
            </div>
            <div class="form-group">
                <label>Visi Hidup</label>
                <textarea class="form-control" rows="3" placeholder="Visi Hidup"></textarea>
            </div>
        </div>

        <div class="box-footer">
            <input type="hidden" name="action" value="datapersonal">
            <button type="submit" class="btn btn-success">Submit</button>
        </div>
    </form>
</div>
<!-- /.box -->

<script>
    $('#tokenfield').tokenfield();
</script>